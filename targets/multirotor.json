{
  "profile": "Multirotor OpenTX Sound Pack Addon",
  "directory": "./out/SOUNDS/en/",
  "audio_format": {
    "channels": 1,
    "sample_freq": 32000,
    "gain": 18
  },
  "filename_validaton": "^\\w{1,6}\\.\\w{1,3}$",
  "sounds": {
    "angle.wav"     : "angle",
    "acro.wav"      : "acro",
    "horz.wav"      : "horizon",
    "arm.wav"       : "arming",
    "arm_n.wav"     : "disarmed",
    "beep.wav"      : "beeper",
    "beep_n.wav"    : "beeper ^32:1 off",
    "bat_l.wav"     : "battery ^32:1 low_a",
    "bat_c.wav"     : "battery ^32:1 critical_a",
	"lts.wav"       : "lights",
    "lts_n.wav"     : "lights ^32:1 off",
    "bb.wav"        : "blackbox",
    "bb_n.wav"      : "blackbox ^32:1 off",
    "airm.wav"      : "air ^32:3 mode -32:1",
    "airm_n.wav"    : "air ^32:3 mode -32:1 off",
    "alth.wav"      : "altitude ^32:1 hold",
    "altf.wav"      : "altitude ^32:1 free",
    "hdgh.wav"      : "heading ^32:1 hold",
    "hdgf.wav"      : "heading ^32:1 free",	
    "celv_l.wav"    : "cell ^16:1 voltage ^32:1 low_a",
    "celv_c.wav"    : "cell ^16:1 voltage ^32:1 critical_a"
  }
}
